{
    "name": "lara-care/schema-visualizer",
    "description": "Visualize and analyze Laravel DB schema and models",
    "type": "library",
    "license": "MIT",
    "authors": [
      {
        "name": "Jery FOTO",
        "email": "jeryfoto@gmail.com"
      }
    ],
    "require": {
      "php": "^8.1",
      "laravel/framework": "^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0",
      "nikic/php-parser": "*",
      "livewire/livewire": "*"
    },
    "autoload": {
      "psr-4": {
        "SchemaVisualizer\\": "src/"
      }
    },
    "extra": {
      "laravel": {
        "providers": [
          "SchemaVisualizer\\SchemaVisualizerServiceProvider"
        ]
      }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
  }
